{
  "name": "scummvm",
  "components": {
    "scummvm-vnc": {
      "image": "registry.giantswarm.io/puja/scummvm",
      "ports": [
        5900
      ],
      "domains": {
        "5900/tcp": [
          "scummvm-puja.gigantic.io"
        ]
      },
      "env": {
        "HOME": "/"
      },
      "entrypoint": "x11vnc",
      "args": ["-forever", "-create"]
    }
  }
}
